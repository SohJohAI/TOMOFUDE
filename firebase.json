{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ],
    "source": "functions"
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "emulators": {
    "auth": {
      "port": 9099
    },
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "ui": {
      "enabled": true
    }
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "tomofude-dev",
          "appId": "1:474299308134:android:3afd083292a660b32375a7",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "tomofude-dev",
          "configurations": {
            "android": "1:474299308134:android:3afd083292a660b32375a7",
            "ios": "1:474299308134:ios:d35a044dc60da1942375a7",
            "macos": "1:474299308134:ios:d35a044dc60da1942375a7",
            "web": "1:474299308134:web:fb4c71f111156a392375a7",
            "windows": "1:474299308134:web:0b16bc4c57024f292375a7"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
